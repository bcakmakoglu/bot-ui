{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@types/js-cookie",
      "@types/graphql",
      "vue-apollo/types",
      "@nuxt/types",
      "@nuxtjs/apollo/types",
      "@nuxtjs/toast"
    ]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist",
    "./types/**/*"
  ],
  "files": ["graphql.d.ts"]
}
